#!/bin/sh

set -eu

PKG_USER="stadtgestalten"
DIR_ETC="/etc/grouprise"

if [ -f "/etc/uwsgi/apps-enabled/grouprise.ini" ]; then
    touch "$DIR_ETC/maintenance_mode"
    /usr/bin/stadtctl database_dump --prefix preinst_auto
fi

set +eu

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
